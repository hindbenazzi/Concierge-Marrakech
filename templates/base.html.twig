<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Concierge-Marrakech{% endblock %}</title>
        {% block stylesheets %}
         <!-- GOOGLE FONTS-->
    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Crimson+Text' rel='stylesheet' type='text/css'>
    
    <!-- STYLESHEETS-->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <link href="css/font-awesome.min.css" rel="stylesheet">
    <link href='css/owl.carousel.css' rel="stylesheet">
    <link href='css/style.css' rel="stylesheet">
    <link href='css/responsive.css' rel="stylesheet">
        {% endblock %}
    </head>
    <body>
    
           <!-- Start Header -->
    <header id="header" class="triangle">

      <!-- Menu toggle Mobile -->
      <label id="toggle" class="toggle"></label>

      <!-- Start Banner -->
      {% block banner%}
      {% endblock %}

        <!-- Start Header-Inner -->
        <div class="header-inner cleafix">

          <!-- Start Header-Logo -->
          <div class="header-logo">
            <a href="{{path('app_home')}}">
            {% block headerlogo%}
              <img src="img/logo.png" alt="logo">
              {% endblock %}
            </a>
          </div>
          <!-- End Header-Logo -->

          <!-- Start Header-Tool-Bar -->
          <div class="header-tool-bar">
            <div class="container">
              <div class="row">

                <!-- Start Header-Left -->
                <div class="col-lg-4 col-md-5 col-xs-12 header-left">

                  <!-- Start Header-Contact -->
                  <ul class="custom-list header-contact">
                    <li>
                      <i class="fa fa-phone"></i>
                      <a href="tel:123-456-7890">+212 661216388</a>
                    </li>
                    <li>
                      <i class="fa fa-envelope"></i>
                        <a href="mailto:kechtrans@gmail.com">
                          kechtrans@gmail.com  
                        </a><br><police color:red> {% trans %}we trust our clients No prepayment is needed payment on arrival{% endtrans %}</police>
                    </li>
                  </ul>
                  <!-- End Header-Contact -->

                </div>
                <!-- End Header-Left -->

                <!-- Start Header-Right -->
                <div class="col-lg-4 col-md-5 col-sm-12 col-xs-12 col-lg-offset-4 col-md-offset-2 header-right">

                  <!-- Start Social -->
                  <ul class="header-social custom-list">
                    <li><a href="https://web.facebook.com/semoncef"><i class="fa fa-facebook-square"></i></a></li>
                    <li><a href="https://www.instagram.com/concierge_vip_marrakech/"><i class="fab fa-instagram"></i></a></li>
                  </ul> 
                  <!-- End Social --> &nbsp;&nbsp;&nbsp;&nbsp;
                  {% block lang %}
      
                 <!-- Start Header-Language -->
                  <div class="header-language">
                    <button class="header-btn">
                    <i class="fa fa-globe"></i>
                    EN
                    </button>
                    <nav class="header-form" >
                      <ul class="custom-list" style="width:80px;">
                        <li ><a href="{{path('translation',{locale:'en'})}}"><img style="width:30px; height:30px; " src="img/flags/united-states.png" style="display:inline-block;">EN</a></li>
                        <li ><a href="{{path('translation',{locale:'fr'})}}" ><img style="width:30px; height:30px; " src="img/flags/france.png" style="display:inline-block;">FR</a></li>
                        <li><a href="{{path('translation',{locale:'es'})}}"><img style="width:30px; height:30px; " src="img/flags/spain.png" style="display:inline-block;">ES</a></li>
                        <li><a href="{{path('translation',{locale:'ar'})}}"><img style="width:30px; height:30px; " src="img/flags/morocco.png" style="display:inline-block;">AR</a></li>
                        <li><a href="{{path('translation',{locale:'rus'})}}"><img style="width:30px; height:30px; " src="img/flags/russia.png" style="display:inline-block;">Rus</a></li>
                        <li><a href="{{path('translation',{locale:'chi'})}}"><img style="width:30px; height:30px; "src="img/flags/china.png" style="display:inline-block;">Chi</a></li>
                      </ul>
                    </nav>
                  </div>
                  <!-- End Header-Language -->

                  {% endblock %}

                </div>
                <!-- End Header-Right -->

              </div>
            </div>
          </div>
          <!-- End Header-tool-bar -->

          <!-- Start Header-Nav -->
          <div class="header-nav">
            <div class="container">
              <div class="row">
                <div class="col-lg-12">

                  <!-- Start Nav -->
                  <nav role="navigation">
                    <ul class="nav navbar-nav">
              
                        <li class="has-submenu"><a href="{{path('app_home')}}" >{% trans %}Home{% endtrans %} </a>
                      </li>
                      <li class="has-submenu">
                        <a href="{{path('private_residence')}}">{% trans %}Private residences{% endtrans %}</a>
                      </li>
                        <li class="has-submenu"><a href="{{path('app_privatepalace')}}">{% trans %}Private palace{% endtrans %} </a></li>
                        
                      <li><a href="{{path('app_vipTrips')}}">{% trans %}vip trips{% endtrans %} &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </a></li>
                    
                     
                    </ul>
                    <ul class="nav navbar-nav navbar-right">
                      <li class="has-submenu"><a href="{{path('app_luxuryCars')}}" >{% trans %}luxury cars{% endtrans %} </a></li>
                        <li class="has-submenu"><a href="{{path('concierge_services')}}">{% trans %}concierge services{% endtrans %} </a></li>
                    <li class="has-submenu">
                      <a href="{{path('app_aboutus')}}">{% trans %}About{% endtrans %}</a>
                      
                    </li>
                  
                    
                    <li class="has-submenu">
                      <a href="{{path('app_contact')}}">{% trans %}Contact{% endtrans %}</a>
                    </li>
                  </ul>
                  </nav>
                          <!-- End Nav -->

                </div>
              </div>
            </div>
          </div>
          <!-- End Header-Nav -->

        </div>
        <!-- End Header-Inner -->
 {% block continuebanner %}
        {% endblock %}
        

    </header>
        <main role="main">
        {% block body %}
        {% endblock %}
        </main>
            <!-- Start Footer -->
    <footer id="footer">
      <div class="container">

          <!-- Start Preamble -->
          <div class="col-lg-12 preamble">
          {% block logo %}
            <img src="img/footer-logo.png" alt="">
            {% endblock %}
          </div>
          <!-- End Preamble -->

          <!-- Start Footer-Content -->
          <div class="footer-content">
            <div class="row">

              <!-- Start Widget-About -->
              <div class="col-lg-4 col-md-4 widget widget-about">
                <h5>{% trans %}About Us{% endtrans %}</h5>
                <p>Marrakech concierge</p>
                <p>{% trans %}VIP services{% endtrans %}</p>
                <p> +212 661216388 - kechtrans@gmail.com </p>
              </div>
              <!-- End Widget-About -->

              <!-- Start Widget-News -->
              <div class="col-lg-4 col-md-4 widget widget-news">
                <h5>{% trans %}Latest News{% endtrans %}</h5>
                <ul class="custom-list">
                  <li><a href="#">Marrakech - Dakhla with Royal Air</a></li>
                  <li><a href="#">Golf competition</a></li>
                  <li><a href="#">Marrakech Casinos</a></li>
                </ul>
              </div>
              <!-- End Widget-News -->

              <!-- Start Widget-Newsletter -->
              <div class="col-lg-4 col-md-4 widget widget-newsletter">
                <h5>{% trans %}Newsletter{% endtrans %}</h5>
                <form class="default-form">
                  <input type="text" name="newsletter" placeholder="Your Email Here">
                  <button class="btn light">{% trans %}Sign Up{% endtrans %}</button>
                </form>
              </div>
              <!-- End Widget-Newsletter -->

              <!-- Start Widget-Social -->
              <div class="col-lg-4 col-md-4 widget widget-social">
                <h5>{% trans %}Lets Stay in Touch{% endtrans %}</h5>
                <ul class="custom-list">
                  <li>
                    <a href="https://web.facebook.com/semoncef"><i class="fa fa-facebook-square"></i></a>
                  </li>
                  <li>
                    <li><a href="https://www.instagram.com/concierge_vip_marrakech/"><i class="fab fa-instagram"></i></a></li>
                  </li>
                  
                </ul>
              </div>
              <!-- End Widget-Social -->
            </div>
        </div>
      </div>
      <!-- End Footer-Content -->

      <!-- Start Footer-Copyrights -->
      <div class="footer-copyrights">
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <p>Marrakech Concierge 2019. Powered by <a href="#">IIP</a></p>
            </div>
          </div>
        </div>
      </div>
      <!-- End Footer-Copyrights -->

    </footer>
    <!-- End Footer -->

    <a href="#" class="btn" id="back-to-top"><i class="fa fa-arrow-up"></i></a>

   
        {% block javascripts %}
        <script src="js/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/jquery.vide.min.js"></script>
    <script src="js/jquery-ui-1.10.4.custom.min.js"></script>
    <script src="js/script.js"></script>
    <script src="js/jquery.ba-outside-events.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/jquery.placeholder.js"></script>
<script src="https://kit.fontawesome.com/23bc112b5b.js" crossorigin="anonymous"></script>
        {% endblock %}
    </body>
</html>
